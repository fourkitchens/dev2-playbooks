# nginx
nginx_default_server_name: '"~^(?<domain>[^\.]+)\.local.dev$"'
nginx_default_root: '/home/vagrant/www/$domain'

# Apache
apache_server_admin: 'it@fourkitchens.com'
apache_server_alias: '*.*.webchefs.org'
apache_server_virtual_docroot: '/home/%-4/www/%-3'
apache_server_directory: '/home/*/www/*'
apache_server_name: 'dev1'
apache_server_home_alias: 'webchefs.org'
